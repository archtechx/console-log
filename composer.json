{
    "name": "leanadmin/console-log",
    "description": "consoleLog() for Livewire components",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lean\\ConsoleLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lean\\ConsoleLog\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel.stancl@gmail.com"
        }
    ],
    "require": {
        "livewire/livewire": "^2.3",
        "illuminate/view": "^9.0|^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.0|^2.0",
        "illuminate/support": "^9.0|^10.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0",
        "illuminate/testing": "^9.0|10.0",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest-plugin-livewire": "^1.1|^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lean\\ConsoleLog\\ConsoleLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
